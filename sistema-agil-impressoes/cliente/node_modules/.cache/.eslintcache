     {loading ? (\r\n          <LoadingBox></LoadingBox>\r\n        ) : error ? (\r\n          <MensageBox variant=\"danger\">{error}</MensageBox>\r\n        ) : (\r\n          <div className=\"row top\">\r\n            <div className=\"col-2\">\r\n              <img src={produto.image} alt={produto.image} className=\"large\" />\r\n            </div>\r\n            <div className=\"col-1\">\r\n              <ul>\r\n                <li>{produto.nome}</li>\r\n                <li>\r\n                  <Avaliacao\r\n                    avaliacao={produto.avaliacao}\r\n                    numVisualizacoes={produto.numVisualizacoes}\r\n                  ></Avaliacao>\r\n                </li>\r\n                <li>Preço: ${produto.preco}</li>\r\n                <li>Descrição: {produto.descricao}</li>\r\n              </ul>\r\n            </div>\r\n            <div className=\"col-1\">\r\n              <div className=\"card card-body\">\r\n                <ul>\r\n                  <li>\r\n                    <div className=\"row\">\r\n                      <div>Preço</div>\r\n                      <div className=\"price\">R${produto.preco}</div>\r\n                    </div>\r\n                  </li>\r\n                  <li>\r\n                    <div className=\"row\">\r\n                      <div>Status</div>\r\n                      <div className=\"price\">\r\n                        {produto.quantidadeEmEstoque > 0 ? (\r\n                          <span className=\"sucesso\">'Em estoque' </span>\r\n                        ) : (\r\n                          <span className=\"erro\">'Produto indisponível'</span>\r\n                        )}\r\n                      </div>\r\n                    </div>\r\n                  </li>\r\n\r\n                  {produto.quantidadeEmEstoque > 0 && (\r\n                    <>\r\n                      <li>\r\n                        <div className=\"row\">\r\n                          quantidade\r\n                          <div>\r\n                            <select\r\n                              value={quantidade}\r\n                              onChange={(e) => setQuantidade(e.target.value)}\r\n                            >\r\n                              {[\r\n                                ...Array(produto.quantidadeEmEstoque).keys(),\r\n                              ].map((qtd) => (\r\n                                <option key={qtd + 1} value={qtd + 1}>\r\n                                  {qtd + 1}\r\n                                </option>\r\n                              ))}\r\n                            </select>\r\n                          </div>\r\n                        </div>\r\n                      </li>\r\n                      <li>\r\n                        <button\r\n                          onClick={adicionarItemCarrinhoHandler}\r\n                          className=\"primary block\"\r\n                        >\r\n                          Adicionar ao carrinho\r\n                        </button>\r\n                      </li>\r\n                    </>\r\n                  )}\r\n                </ul>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        )}\r\n        ;\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n","C:\\Users\\agil\\Documents\\GIT e GITHUB\\projetos-react\\sistema-agil-impressoes\\cliente\\src\\reducers\\carrinhoReducers.js",[],"C:\\Users\\agil\\Documents\\GIT e GITHUB\\projetos-react\\sistema-agil-impressoes\\cliente\\src\\reducers\\produtosReducers.js",[],"C:\\Users\\agil\\Documents\\GIT e GITHUB\\projetos-react\\sistema-agil-impressoes\\cliente\\src\\constantes\\constantesCarrinhoCompras.js",[],"C:\\Users\\agil\\Documents\\GIT e GITHUB\\projetos-react\\sistema-agil-impressoes\\cliente\\src\\actions\\produtosActions.js",[],"C:\\Users\\agil\\Documents\\GIT e GITHUB\\projetos-react\\sistema-agil-impressoes\\cliente\\src\\actions\\carrinhoCompraAction.js",[],"C:\\Users\\agil\\Documents\\GIT e GITHUB\\projetos-